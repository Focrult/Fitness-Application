<div class="container">
  <div class="row">
    <div class="col-md-8">
        <div>
            <canvas id="myChart"></canvas>
        </div>
    </div>
    <div class="col-md-4">
      <div class="card">
        <div class="card-header underline">Top 3 Workouts of the Week!</div>
        <div class="card-body">
          <ul>
            {{#each popularWorkouts}}
              <li>{{this.workoutName}} ({{this.count}})</li>
            {{/each}}
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>

<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
<script src="./js/chart.js"></script>
